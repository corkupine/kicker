<% if user_signed_in? %>
<%= simple_form_for(@award) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :date %>
    <%= f.input :award_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
<% else %>
<p class="label alert">You must be logged in to add or edit Awards.</p>
<% end %>

